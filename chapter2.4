#include<stdio.h>
void jolly(void);	//functin prototyping
void nobody(void);	//function prototyping
int main(void)
{
	printf("For he's a jolly good fellow!\n");
	printf("For he's a jolly good fellow!\n");
	jolly();
	nobody();
	
	return 0;
}

void jolly(void)	//define jolly() function
{
	printf("For he's a jolly good  fellow!\n");
}

void nobody(void)	//define nobody() function
{
	printf("Which nobody can deny!");
}
